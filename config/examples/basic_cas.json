{
  "stores": {
    "CAS_MAIN_STORE": {
      "verify": {
        "backend": {
          "compression": {
            "compression_algorithm": {
              "LZ4": {}
            },
            "backend": {
              "s3_store": {
                "region": "us-west-1",
                "bucket": "blaisebruer-cas-store",
                "key_prefix": "test-prefix-cas/",
                "retry": {
                  "max_retries": 6,
                  "delay": 0.3,
                  "jitter": 0.5,
                }
              }
            }
          }
        },
        "verify_size": true,
        "verify_hash": true
      }
    },
    "AC_MAIN_STORE": {
      // "memory": {
      //   "eviction_policy": {
      //     // 100mb.
      //     "max_bytes": 100000000,
      //   }
      // }
      "s3_store": {
        "region": "us-west-1",
        "bucket": "blaisebruer-cas-store",
        "key_prefix": "test-prefix-ac/",
        "retry": {
          "max_retries": 6,
          "delay": 0.3,
          "jitter": 0.1,
        }
      }
    }
  },
  "servers": [{
    "listen_address": "0.0.0.0:50051",
    "services": {
      "cas": {
        "main": {
          "cas_store": "CAS_MAIN_STORE"
        }
      },
      "ac": {
        "main": {
          "ac_store": "AC_MAIN_STORE"
        }
      },
      "capabilities": {},
      "execution": {},
      "bytestream": {
        "cas_stores": {
          "main": "CAS_MAIN_STORE",
        },
        // This value was chosen only because it is a common mem page size.
        "write_buffer_stream_size": 2e6, // 2mb.
        "read_buffer_stream_size": 2e6, // 2mb.
        // According to https://github.com/grpc/grpc.github.io/issues/371 16KiB - 64KiB is optimal.
        "max_bytes_per_stream": 64000, // 64kb.
      }
    }
  }]
}
