{
  "stores": [
    {
      "name": "CAS_MAIN_STORE",
      "verify": {
        "backend": {
          "name": "cas-dedup",
          "dedup": {
            "index_store": {
              "name": "cas-index",
              "fastSlow": {
                "fast": {
                  "name": "cas-index-fast",
                  "filesystem": {
                    "content_path": "/tmp/nativelink/data/content_path-index",
                    "temp_path": "/tmp/nativelink/data/tmp_path-index",
                    "eviction_policy": {
                      "max_bytes": "500Mb",
                    }
                  }
                },
                "slow": {
                  "name": "cas-index-slow",
                  "s3": {
                    "region": "eu-north-1",
                    "bucket": "crossplane-bucket-af79aeca9",
                    "key_prefix": "test-prefix-index/",
                    "retry": {
                      "max_retries": 6,
                      "delay": 0.3,
                      "jitter": 0.5
                    },
                  }
                }
              }
            },
            "content_store": {
              "name": "cas-content",
              "compression": {
                "compression_algorithm": {
                  "lz4": {}
                },
                "backend": {
                  "name": "cas-content-backend",
                  "fastSlow": {
                    "fast": {
                      "name": "cas-content-backend-fast",
                      "filesystem": {
                        "content_path": "/tmp/nativelink/data/content_path-content",
                        "temp_path": "/tmp/nativelink/data/tmp_path-content",
                        "eviction_policy": {
                          "max_bytes": "2Gb",
                        }
                      }
                    },
                    "slow": {
                      "name": "cas-content-backend-slow",
                      "s3": {
                        "region": "eu-north-1",
                        "bucket": "crossplane-bucket-af79aeca9",
                        "key_prefix": "test-prefix-dedup-cas/",
                        "retry": {
                          "max_retries": 6,
                          "delay": 0.3,
                          "jitter": 0.5
                        },
                      }
                    }
                  }
                }
              }
            }
          }
        },
        "verify_size": true,
        "verify_hash": true
      }
    }, {
      "name": "AC_MAIN_STORE",
      "fastSlow": {
        "fast": {
          "name": "ac-fast",
          "memory": {
            "eviction_policy": {
              "max_bytes": "100Mb",
            }
          }, // TODO(aaronmondal): Broken??
          "filesystem": {
            "content_path": "/tmp/nativelink/data/content_path-ac",
            "temp_path": "/tmp/nativelink/data/tmp_path-ac",
            "eviction_policy": {
              // 500mb.
              "max_bytes": 500000000
            }
          }
        },
        "slow": {
          "name": "ac-slow",
          "s3": {
            "region": "eu-north-1",
            "bucket": "crossplane-bucket-af79aeca9",
            "key_prefix": "test-prefix-ac/",
            "retry": {
              "max_retries": 6,
              "delay": 0.3,
              "jitter": 0.5
            },
          }
        }
      }
    }
  ],
  "schedulers": [
    {
      "name": "MAIN_SCHEDULER",
      "simple": {
        "supported_platform_properties": {
          "cpu_count": "minimum",
          "memory_kb": "minimum",
          "network_kbps": "minimum",
          "disk_read_iops": "minimum",
          "disk_read_bps": "minimum",
          "disk_write_iops": "minimum",
          "disk_write_bps": "minimum",
          "shm_size": "minimum",
          "gpu_count": "minimum",
          "gpu_model": "exact",
          "cpu_vendor": "exact",
          "cpu_arch": "exact",
          "cpu_model": "exact",
          "kernel_version": "exact",
          "docker_image": "priority",
          "lre-rs": "priority"
        }
      }
    }
  ],
  "servers": [{
    "listener": {
      "http": {
        "socket_address": "0.0.0.0:50051"
      }
    },
    "services": {
      "cas": {
        "main": {
          "cas_store": "CAS_MAIN_STORE"
        }
      },
      "ac": {
        "main": {
          "ac_store": "AC_MAIN_STORE"
        }
      },
      "execution": {
        "main": {
          "cas_store": "CAS_MAIN_STORE",
          "scheduler": "MAIN_SCHEDULER"
        }
      },
      "capabilities": {
        "main": {
          "remote_execution": {
            "scheduler": "MAIN_SCHEDULER"
          }
        }
      },
      "bytestream": {
        "cas_stores": {
          "main": "CAS_MAIN_STORE"
        }
      },
      "health": {}
    }
  }]
}
