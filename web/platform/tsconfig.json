{
  "extends": "astro/tsconfigs/strictest",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@components/*": ["./src/components/*"],
      "@qwik/*": ["./src/components/qwik/*"],
      "@react/*": ["./src/components/react/*"],
      "@assets/*": ["./src/assets/*"],
      "@styles/*": ["./styles/*"],
      "@utils/*": ["./utils/*"]
    },
    "types": ["bun"],
    "jsx": "preserve",
    "jsxImportSource": "@builder.io/qwik",
  }
}
